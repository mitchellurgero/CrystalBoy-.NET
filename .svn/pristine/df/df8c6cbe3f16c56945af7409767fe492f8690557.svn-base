using System;

namespace CrystalBoy.Emulation
{
	[Flags]
	public enum GameBoyKeys : byte
	{
		None = 0x00,
		Right = 0x01,
		Left = 0x02,
		Up = 0x04,
		Down = 0x08,
		A = 0x10,
		B = 0x20,
		Select = 0x40,
		Start = 0x80,
		All = 0xFF
	}
}
